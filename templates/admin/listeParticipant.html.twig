{% extends 'base.html.twig' %}

{% block title %}{% endblock %}
{% block titrepage %}Liste des participants{% endblock %}
{% block body %}
    {% for participant in participants %}
        <p>{{ participant.pseudo }}</p>
        <p>{{ participant.lastname | upper }} {{ participant.firstname }}</p>
        <a href="{{ path('app_profil_modification' , {'id' : participant.id} )}}">Modifier le participant</a>
        <a href="{{ path('app_admin_participant_supprimer', {'id': participant.id}) }}">Supprimer le participant</a>
    {% endfor %}
{% endblock %}